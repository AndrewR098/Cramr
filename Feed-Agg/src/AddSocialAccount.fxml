<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" style="-fx-background-color: #353839;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.AddSocialController">
   <children>
      <TabPane layoutY="318.0" prefHeight="382.0" prefWidth="700.0" side="BOTTOM" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Twitter">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="313.0" prefWidth="700.0">
                     <children>
                        <Button fx:id="twitterLoginButton" layoutX="328.0" layoutY="152.0" mnemonicParsing="false" onAction="#handle" text="Login">
                           <font>
                              <Font name="Segoe UI Light" size="12.0" />
                           </font>
                        </Button>
                        <ImageView fitHeight="13.0" fitWidth="66.0" layoutX="317.0" layoutY="89.0">
                           <image>
                              <Image url="@resources/twitter.png" />
                           </image>
                        </ImageView>
                        <Label layoutX="257.0" layoutY="118.0" text="Login through Twitter website" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI" size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Reddit">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="102.0" layoutY="113.0" text="Subreddit: (all lowercase, no spaces)  r/" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Light" size="12.0" />
                           </font>
                        </Label>
                        <ImageView fitHeight="31.0" fitWidth="78.0" layoutX="326.0" layoutY="63.0">
                           <image>
                              <Image url="@resources/reddit-logo-white.png" />
                           </image>
                        </ImageView>
                        <TextField fx:id="subredditText" layoutX="298.0" layoutY="110.0" onAction="#handle" />
                        <Button fx:id="addSubredditButton" layoutX="321.0" layoutY="152.0" mnemonicParsing="false" onAction="#handle" text="Add subreddit">
                           <font>
                              <Font name="Segoe UI Light" size="12.0" />
                           </font>
                        </Button>
                        <Button fx:id="removeSubredditButton" layoutX="311.0" layoutY="190.0" mnemonicParsing="false" onAction="#handle" text="Remove subreddit">
                           <font>
                              <Font name="Segoe UI Light" size="12.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab text="Facebook">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ImageView fitHeight="22.0" fitWidth="66.0" layoutX="317.0" layoutY="79.0">
                           <image>
                              <Image url="@resources/facebook-logo-white-full-transparent.png" />
                           </image>
                        </ImageView>
                        <Label layoutX="292.0" layoutY="120.0" text="Coming Soon..." textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Light" size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
      <Label layoutX="222.0" layoutY="124.0" text="Add An Account" textFill="WHITE">
         <font>
            <Font name="Segoe UI Light" size="36.0" />
         </font>
      </Label>
      <ImageView fitHeight="22.0" fitWidth="66.0" layoutX="454.0" layoutY="223.0">
         <image>
            <Image url="@resources/facebook-logo-white-full-transparent.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="31.0" fitWidth="78.0" layoutX="315.0" layoutY="218.0">
         <image>
            <Image url="@resources/reddit-logo-white.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="13.0" fitWidth="66.0" layoutX="173.0" layoutY="229.0">
         <image>
            <Image url="@resources/twitter.png" />
         </image>
      </ImageView>
      <Button fx:id="homeButton" layoutX="608.0" layoutY="32.0" mnemonicParsing="false" onAction="#handle" style="-fx-background-color: transparent;" text="&lt;HOME" textFill="#fbe870">
         <font>
            <Font name="Segoe UI Bold" size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
