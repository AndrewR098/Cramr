<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.SplashController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="700.0">
         <image>
            <Image url="@resources/65322-landscape-nature-mountain-sunset-sunrise-sunlight-blurred-Nepal-Himalayas-climbing-rock.jpg" />
         </image>
      </ImageView>
      <Button fx:id="splashButton" layoutX="263.0" layoutY="309.0" mnemonicParsing="false" onAction="#handle" prefHeight="31.0" prefWidth="175.0" style="-fx-background-color: black;" text="Login" textFill="WHITE">
